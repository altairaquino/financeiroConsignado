FROM VW_ENTIDADE
LEFT JOIN VW_CONTRATO ON
   ENNCODG = CTNCGEN AND
   EXTRACT(MONTH FROM CTDPGTO) = EXTRACT(MONTH FROM CAST('NOW' AS DATE)) AND
