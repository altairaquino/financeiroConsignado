	<queryString><![CDATA[SELECT
   ANNCGEN,
   ENCNOME,
   ANNMETA META_MES,
   (SELECT COALESCE(SUM(CTYVALR),0) FROM CONTRATO
   WHERE CTNCGEN = ANNCGEN AND 
	EXTRACT(MONTH FROM CTDCADT) = EXTRACT(MONTH FROM CAST($P{DATA} AS DATE)) AND
	EXTRACT(YEAR FROM CTDCADT) = EXTRACT(YEAR FROM CAST($P{DATA} AS DATE))
   AND CTNCGSC = 6) PRODUCAO,
   ((SELECT COALESCE(SUM(CTYVALR),0) FROM CONTRATO
   WHERE CTNCGEN = ANNCGEN AND 
	EXTRACT(MONTH FROM CTDCADT) = EXTRACT(MONTH FROM CAST($P{DATA} AS DATE)) AND
	EXTRACT(YEAR FROM CTDCADT) = EXTRACT(YEAR FROM CAST($P{DATA} AS DATE))
   AND CTNCGSC = 6)/ANNMETA*100)  PERC_PROD,
   (SELECT COALESCE(SUM(CTYVALR),0) FROM CONTRATO
   WHERE CTNCGEN = ANNCGEN AND 
	EXTRACT(MONTH FROM CTDCADT) = EXTRACT(MONTH FROM CAST($P{DATA} AS DATE)) AND
	EXTRACT(YEAR FROM CTDCADT) = EXTRACT(YEAR FROM CAST($P{DATA} AS DATE))
   AND CTNCGSC != 6) OUTROS,
   ((SELECT COALESCE(SUM(CTYVALR),0) FROM CONTRATO
   WHERE CTNCGEN = ANNCGEN AND 
	EXTRACT(MONTH FROM CTDCADT) = EXTRACT(MONTH FROM CAST($P{DATA} AS DATE)) AND
	EXTRACT(YEAR FROM CTDCADT) = EXTRACT(YEAR FROM CAST($P{DATA} AS DATE))
   AND CTNCGSC != 6)/ANNMETA*100) PERC_OUTROS
 FROM
   ANGARIADOR INNER JOIN ENTIDADE ON
      ENNCODG = ANNCGEN
 WHERE ENLATIV = 'T'
 AND ANLAGEN = 'T'
 ORDER BY ENCNOME;]]></queryString>

