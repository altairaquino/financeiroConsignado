	<queryString><![CDATA[SELECT
   E1.ENCNOME,
   E1.ENCDOCM,
   EMPCCARG,
   SUM((CTYVALR * CCNPERC / 100.0))SOMA
 FROM
   COMISSAOCONTRATO
   INNER JOIN CONTRATO ON
     CTNCODG = CCNCGCT
   INNER JOIN SITUACAOCONTRATO ON
     CTNCGSC = SCNCODG
   INNER JOIN ENTIDADE E1 ON
     E1.ENNCODG = CCNCGEN
   INNER JOIN ENTIDADETIPO ON
     E1.ENNCODG = ETNCGEN
   INNER JOIN EMPREGADO ON
     E1.ENNCODG = EMPNCGCEN
 WHERE ETNCGTE = 3
 AND SCLPGCM = 'T'
 AND CTDPGTO IS NOT NULL
 AND CTDPGTO BETWEEN $P{DATA_INICIO} AND $P{DATA_FIM}
 GROUP BY 1,2,3
 ORDER BY E1.ENCNOME]]></queryString>

