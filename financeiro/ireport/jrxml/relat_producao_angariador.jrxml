	<queryString><![CDATA[SELECT
   CTNCGEN,
   CTCNMAN,
   SUM(CTYVALR) SOMA_PRODUCAO,
   SUM(CTYCOMS) SOMA_COMISSAO,
   SUM(CTYEXAT) SOMA_EXATA,
   SUM(CTYSPRE) SPREAD
 FROM
   VW_CONTRATO
 WHERE
   CTDPGTO BETWEEN $P{DATA_INICIO} AND $P{DATA_FIM}
   AND CTLPGCM = 'T'
   AND EXISTS (SELECT * FROM ANGARIADOR WHERE ANNCGEN = CTNCGEN AND ANNCGTN = 3)
 GROUP BY CTNCGEN, CTCNMAN
 ORDER BY CTCNMAN]]></queryString>

