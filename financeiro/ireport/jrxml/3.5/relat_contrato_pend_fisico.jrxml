	<queryString><![CDATA[SELECT
ENCNOME,
RESULT.*
FROM
(
SELECT
CTNCGEN,
COUNT(IIF(DAYS > 8,CTYVALR, NULL))QTD_PEND,
COUNT(*)QTD_TOTAL,
COALESCE(SUM(IIF(DAYS > 8,CTYVALR, NULL)),0)
VALOR_PEND,
COALESCE(SUM(CTYVALR),0)VALOR_TOTAL,
COUNT(IIF(DAYS > 8,CTYVALR, NULL))/
 CAST(COUNT(*) AS DOUBLE PRECISION)PERC
FROM(
SELECT CONTRATO.*,
       (SELECT DAYS FROM SP_DAYSBETWEEN('NOW', CTDPGTO))
FROM CONTRATO
)
WHERE CTLFISI = 'F' AND
      CTDPGTO IS NOT NULL AND
      CTNCGSC = 6
GROUP BY CTNCGEN
HAVING COUNT(IIF(DAYS > 8,CTYVALR, NULL)) > 0) RESULT INNER JOIN ENTIDADE ON
CTNCGEN = ENNCODG
ORDER BY ENCNOME]]></queryString>

