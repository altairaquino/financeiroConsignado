	<queryString><![CDATA[SELECT
   CRMNCGCR,
   CRCDESC,
   MOVNCGGPC,
   MOVCDCGPC,
   MOVNCGCOG,
   MOVCDCCOG,
   SUM(MOVYVALR / (SELECT COUNT(*) 
 			FROM CENTROMOVCAIXA WHERE
                	CRMNMOVC = MOVNCODG)) VALOR
 FROM
   CENTROMOVCAIXA INNER JOIN VW_MOVIMENTO ON
     MOVNCODG = CRMNMOVC
   INNER JOIN CENTROCUSTO ON
     CRMNCGCR = CRNCODG     
 WHERE MOVLATIV = 'T'
 AND MOVCTPGPC = 'E'
 AND MOVLTIPO = 'M'
 AND MOVDDATA BETWEEN $P{DATA_INICIO} AND $P{DATA_FIM}
GROUP BY
   CRMNCGCR,
   CRCDESC,
   MOVNCGGPC,
   MOVCDCGPC,
   MOVNCGCOG,
   MOVCDCCOG
ORDER BY CRMNCGCR, MOVNCGGPC, MOVCDCCOG]]></queryString>

